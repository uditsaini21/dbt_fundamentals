{{
  config(
    materialized='table'
  )
}}
with SMP_USER_DATA  as (
   SELECT _MODIFIED AS MODIFIED_DATE_NEW,
USER_ID,
FULL_NAME,
PREFERRED_SOCIAL_MEDIA_PLATFORM AS SMP_FIRST_PREFERRED,
SECOND_PREFERRED_SOCIAL_MEDIA_PLATFORM AS SMP_SECOND_PREFERRED,
AVG_MONTHLY_USAGE_IN_HRS_ AS AVG_MONTHLY_USAGE_IN_HRS,
CONSUMPTION_LEVEL,
SURVEY_DATE,
AGE_GROUP,
GENDER,
STATE FROM "FIVETRAN_DATABASE"."SMP"."SMP_USER_DATA"
)
select *
from SMP_NEW_FILE1